<%= simple_form_for(@room) do |f| %>
  <%= f.input :name %>
 <%= f.input :facility_name %>
 <%= f.input :category %>
  <%= f.input :equipment %>
  <%= f.input :location %>
   <%= f.input :price_per_hour %>
    <%= f.input :capacity %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
